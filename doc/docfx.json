{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.sln"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
		      "src": ".."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "../packages/msdn.4.5.2.0.1.0-alpha-1611021200/content/msdn.4.5.2.zip"
    ],
    "globalMetadata" : {
      "_appTitle": "IPFS Core documentation",
      "_appFooter": "Generated by DocFX",
	  "_appFaviconPath": "images/ipfs-favicon.ico",
      "_appLogoPath":  "images/ipfs-logo.svg"
	  },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
